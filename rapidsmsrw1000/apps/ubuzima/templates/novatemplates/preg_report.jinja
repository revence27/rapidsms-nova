{% extends request.base_template %}
{% block title %}Pregnancy Report{% endblock %}

{% block content %}
<table>
  <thead>
    <tr>
      <th>Province</th>
      <th># of Registered Pregnancies</th>
      <th>High-Risk Pregnancies</th>
      <th>Expected Deliveries</th>
      <th>High-Risk Expected Deliveries</th>
      <th>Due on {{  filters.period.end  }}</th>
      <th>To Deliver With 2 Weeks</th>
      <th>High-Risk due to Deliver Within 2 Weeks</th>
    </tr>
  </thead>
  <tbody>
    {%  for prv in filters.province %}
    <tr>
      <td>{{  prv.name }}</td>
      <td>{{  prv.registered_pregnancies }}</td>
      <td>{{  prv.high_risk_pregnancies }}</td>
      <td>{{  prv.expected_deliveries }}</td>
      <td>{{  prv.expected_high_risk }}</td>
      <td>{{  prv.delivering_on_end }}</td>
      <td>{{  prv.deliver_in_2_weeks }}</td>
      <td>{{  prv.deliver_soon_high_risk }}</td>
    </tr>
    {%  endfor  %}
  </tbody>
</table>
{% endblock %}
